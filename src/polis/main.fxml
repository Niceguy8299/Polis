<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import java.net.URL?>
<StackPane alignment="BOTTOM_CENTER" prefHeight="685" prefWidth="1600" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="polis.MainCompanion">
   <stylesheets>
      <URL value="@/polis/main.css"/>
   </stylesheets>
   <children>
      <StackPane fx:id="viewportStackPane">
      </StackPane>
      <HBox fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="65.0" prefWidth="1500.0">
         <children>
            <HBox id="panel" alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="910.0" spacing="50.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="90.0" prefWidth="400.0" spacing="13.0">
                     <children>
                        <Button id="homeButton" mnemonicParsing="false" prefHeight="61.0" prefWidth="93.0" text="RESIDENCY" />
                        <Button id="industryButton" mnemonicParsing="false" prefHeight="61.0" prefWidth="93.0" text="INDUSTRY" />
                        <Button id="shoppingButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="93.0" text="SHOPPING" />
                        <Button id="roadButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="93.0" text="ROADS" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="220.0" spacing="13.0">
                     <children>
                        <Button id="bulldozerButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="93.0" text="DOZER" />
                        <Button id="nukeButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="93.0" text=":)" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="90.0" prefWidth="100.0" spacing="13.0">
                     <children>
                        <Button id="selectButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="93.0" text="SELECTOR" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="735.0" spacing="50.0">
               <children>
                  <HBox id="panel" alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="110.0">
                     <children>
                        <Button id="playButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="93.0" text="PLAY" />
                     </children>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </HBox>
               </children>
            </HBox>
         </children>
         <StackPane.margin>
            <Insets bottom="50.0" />
         </StackPane.margin>
      </HBox>
   </children>
</StackPane>
